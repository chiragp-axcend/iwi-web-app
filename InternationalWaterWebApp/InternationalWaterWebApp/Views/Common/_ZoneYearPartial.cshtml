@using InternationalWaterWebApp.Library.ViewModel
@using InternationalWaterWebApp.Library.Common

@model ZoneYearDetails
@{
    ViewBag.FieldName = Model.Field_Name;
}
@using (Html.BeginForm("SaveFinancialData", "Common", FormMethod.Post, new { @id = "form-" + Model.Management_Zone_Id + "-" + Model.Year + "-" + Model.ZydId, @class = "form-horizontal my-form-class" }))
{
    <div class="row g-0">
        <div class="col-lg-8">
            <div class="LftFormBar p-4">
                    @if(Model.FieldOptionId != 1)
                    {
                    <p class="alert-warning border p-1">For any structural practices, management practices, or cover crops that you have applied to your field, please adjust the financial plan fields accordingly.</p>
                    }
                <div class="accordion" id="accordionExample">
                    <div class="accordion-item mb-4">
                        <h2 class="accordion-header" id="headingOne">
                            @Html.Hidden("Fieldid", ViewBag.FieldId, new { })
                            @Html.Hidden("Name", ViewBag.FieldName, new { })
                            <button class="accordion-button accoBorRadi" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                Zone Details <img src="~/images/Icon feather-chevron-down.png" alt="">
                            </button>
                        </h2>
                        <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
                            <div class="accordion-body tab-content-zone">
                                <div class="row mb-2">
                                    <div class="col-md-5 AccordInnerHead">
                                        Crop:
                                    </div>
                                    <div class="col-md-6">
                                        @Html.HiddenFor(x => x.Management_Zone_Id)
                                        @Html.HiddenFor(x => x.Year)
                                        @Html.HiddenFor(x => x.ZydId)
                                        @Html.HiddenFor(x => x.FieldOptionId)
                                        @* @Html.HiddenFor(x => x.IsValuesChanged, new { id = "isValueChangeFinancialEdit-" + Model.Management_Zone_Id + "-" + Model.Year + "-" + Model.ZydId }) *@
                                        <span class="crop-name"> @Html.DisplayFor(x => x.CropName, new { htmlAttributes = new { @class = "crop-name" }} ) </span>
                                        @*  <span class="crop-name"> @Model.CropName </span>*@
                                    </div>
                                </div>
                                <div class="row mb-2">
                                    <div class="col-md-5 AccordInnerHead">
                                        Primary Tillage:
                                    </div>
                                    <div class="col-md-6">
                                        @Html.DisplayFor(x => x.TillageName)
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-5 AccordInnerHead">
                                        Yield Designation:
                                    </div>
                                    <div class="col-md-6">
                                        @Html.DisplayFor(x => x.Yield)
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="accordion-item mb-4">
                        <h2 class="accordion-header mb-3" id="headingTwo">
                            <button class="accordion-button collapsed accoBorRadi"
                                    type="button" data-bs-toggle="collapse"
                                    data-bs-target="#collapseTwo"
                                    aria-expanded="false"
                                    aria-controls="collapseTwo">
                                <span class="w-75">Gross Revenue</span>
                                <span class="TotalGrossRevenue"></span> <img src="~/images/Icon feather-chevron-down.png" alt="">
                            </button>
                        </h2>
                        <div id="collapseTwo"
                             class="accordion-collapse collapse"
                             aria-labelledby="headingTwo"
                             data-bs-parent="#accordionExample">
                            <div class="accordion-body">
                                @Html.HiddenFor(x => x.Management_Zone_Id)
                                @Html.HiddenFor(x => x.Year)
                                @Html.HiddenFor(x => x.Field_Id)
                                @Html.HiddenFor(x => x.Field_Name)
                                @Html.HiddenFor(x => x.ZydId)
                                @Html.HiddenFor(x => x.DataQualityGrade,new { id="DataQualityGrade-" + Model.Management_Zone_Id + "-" + Model.Year + "-" +Model.ZydId})
                                @Html.Hidden("ManagementZoneAndYear", $"{Model.Management_Zone_Id}-{Model.Year}-{Model.ZydId}")
                                @*TotalProductReturn*@
                                <div class="align-items-center border-3 border-bottom mQuryMargin mb-3 row">
                                    <div class="col-5 p-0 mb-2">
                                        <div class="AccordInnerHead totalGren">
                                            Total Product Return
                                        </div>
                                    </div>
                                    <div class="col-6 InptRgt mb-2">
                                        <div class="row align-items-center">
                                            <div class="col-8 totalGren">
                                                <span>$/acre</span>
                                                <span id="TotalProductReturn"></span>
                                                @Html.HiddenFor(x => x.TotalProductReturn, new{@id="TotalProductReturnInput"})
                                            </div>
                                            <div class="col-lg-2 col-4">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Acres*@
                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Acres <span class="Complsry me-1">*</span>
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Field size based on the field boundary stored within the database. Cultivated area may be less than the field size."></i>

                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">

                                        <div class="row align-items-center">
                                            <div class="col-2">
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.Acres, new { id="Acres-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, type = "text", placeholder="2.5", onblur="sendInputValue(this)", @class = "form-control disabler validator maks",@readonly = "readonly"})
                                                </div>
                                            </div>
                                            <div class="col-1">acres</div>
                                        </div>
                                        <br>
                                    </div>
                                </div>
                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Expected Yield   <span class="Complsry me-1">*</span>
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Yield per acre (e.g., bushel, dry weight) within the management zone. Use the field average assuming typical weather conditions and timely plant/harvest dates if lacking a specific value for the management zone in the selected year."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.ExpectedYield, new { @id = "ExpectedYield-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, type = "text",  onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "$" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.ExpectedYield, "" , new {  @class = "alert-danger" })<br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Renter % share
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Your share of the yield."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">%</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group w-50">
                                                    @Html.TextBoxFor(x => x.OperatorShare, new { maxlength = "3", @id = "OperatorShare-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, type = "number", onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "%" })
                                                    @*@Html.TextBoxFor(x => x.OperatorShare, new { @id = "OperatorShare-" + Model.Management_Zone_Id + "-" + Model.Year, type = "number", onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "%", pattern = "[0-9]{2}", maxlength = "4" })*@
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.OperatorShare, "" , new {  @class = "alert-danger" })
                                        </div>
                                        <br>
                                    </div>
                                </div>

                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Commodity Sale Price <span class="Complsry me-1">*</span>
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Dollars per unit yield (e.g., bushel, dry weight). Use the field average assuming typical weather conditions and timely plant/harvest dates if lacking a specific value for the management zone in the selected year."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.CommoditySalePrice, new { @id = "CommoditySalePrice-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, type = "text",  onblur="sendInputValue(this)", @class = "form-control disabler validator  mask", placeholder = "$"})
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.CommoditySalePrice, "" , new {  @class = "alert-danger" })<br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row align-items-center mb-4 pb-4">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Other Product Revenue <span class="Complsry me-1">*</span>
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Other revenue received on a yield per acre basis."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.OtherProductReturn, new { @id = "OtherProductReturn-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, type = "text",  onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "$" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.OtherProductReturn, "" , new {  @class = "alert-danger" })<br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*TotalGrossRevenue*@
                                <div class="align-items-center bodrGren mQuryMargin mb-3 row">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead totalGren d-flex align-items-center">
                                            Other Revenue
                                        </div>
                                    </div>
                                    <div class="InptRgt col-md-7 mb-2">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @*<span id="TotalGrossRevenue"></span>*@
                                                    @{
                                                        string clsTGR = "TotalGrossRevenue-" + Model.Management_Zone_Id + "-" + Model.Year + "-" + Model.ZydId;
                                                    }
                                                    @*<input type="number" name="TotalGrossRevenue" class="form-control @(clsTGR)">*@
                                                    @*@Html.TextBoxFor(model => model.TotalGrossRevenue, new { @id="TotalGrossRevenue", @class = "form-control" ,onblur="sendInputValue(this)"})*@
                                                    @Html.TextBoxFor(x => x.TotalGrossRevenue, new { @id="TotalGrossRevenue-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler " + clsTGR+ " mask mb-1", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            <div class="col-2">
                                                <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="To make fields editable,
                                                clear all of the textboxes
                                                above "></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Hedge Gain/Loss*@
                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Hedge Gain/Loss
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Difference between contract and actual (futures) sale price."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.HedgeGainLoss, new { @id = "HedgeGainLoss-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.HedgeGainLoss, "" , new {  @class = "alert-danger" }) <br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Crop Insurance Payment*@
                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Crop Insurance Payment
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Revenue from crop insurance."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.CropInsurancePayments, new { @id = "CropInsurancePayments-" + Model.Management_Zone_Id + "-" + Model.Year + "-" + Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.CropInsurancePayments, "" , new {  @class = "alert-danger" }) <br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Stewardship Payments*@
                                <div class="row align-items-center">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Stewardship Payments
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Revenue for conservation related activities (e.g. cover crop cost share; implementing conservation practice)."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.StewardshipPayments, new { @id = "StewardshipPayments-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.StewardshipPayments, "" , new {  @class = "alert-danger" }) <br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Government Payments*@
                                <div class="row align-items-center mb-4">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Government Payments
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Revenue from government payments (e.g., program payments; subsidy)."></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.GovernmentPayments, new { @id = "GovernmentPayments-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler validator mask", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.GovernmentPayments, "" , new {  @class = "alert-danger" }) <br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="accordion-item mb-4">
                        <h2 class="accordion-header" id="headingThree">
                            <button class="accordion-button collapsed accoBorRadi"
                                    type="button" data-bs-toggle="collapse"
                                    data-bs-target="#collapseThree"
                                    aria-expanded="false"
                                    aria-controls="collapseThree">
                                <span class="w-75" style="display: flex; align-items: center;">
                                    Direct Expenses   <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Sometimes called variable expenses because they increase or decrease according to the number of production units (e.g., acres planted, these expenses are incurred only when agricultural production occurs. Direct expenses are $0 in the absence of producing a crop or raising livestock. Common direct expenses include labor, machinery fuel, oil, lubricants and repairs, including labor and fuel costs required to prepare for planting and interest payments for operating capital. &#10;
                                    A commonly overlooked direct expense is the opportunity cost for family labor, management, land and equity capital. Opportunity costs are the benefits you and your operation “miss out on” when choosing an enterprise, such as corn, over an alternative, such as soybeans. For example, although you may not write yourself a paycheck every other week, your labor and management time has value – or an opportunity cost. &#10;
                                  To calculate the value of your management and labor, consider the wage or salary you could be earning working somewhere else. Additionally, include the value of unpaid family labor, which the wage they could be earning working somewhere else."></i>
                                </span>

                                <span class="TotalDirectExpense"></span>
                                <img src="~/images/Icon feather-chevron-down.png" alt="">
                            </button>
                        </h2>
                        <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree" data-bs-parent="#accordionExample">
                            <div class="accordion-body">
                                <div class="align-items-center border-3 border-bottom mQuryMargin mb-2 row">
                                    <div class="col-5 mb-2">
                                        <div class="AccordInnerHead totalGren">
                                            Seed Expenses
                                        </div>
                                    </div>
                                    <div class="col-6 InptRgt mb-2">
                                        <div class="row align-items-center">
                                            <div class="col-lg-10 col-8 totalGren">
                                                <span>$/acre</span>
                                                <span id="TotalSeedrExpenses"></span>
                                            </div>
                                            <div class="col-lg-2 col-4">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*seed*@
                                <div class="row align-items-center mb-3">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Seed
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Seed"></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">

                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.Seed, new { @id = "Seed-" + Model.Management_Zone_Id + "-" + Model.Year + "-" +Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler validator mask small-6-col", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            @Html.ValidationMessageFor(model => model.Seed, "" , new {  @class = "alert-danger" }) <br>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*fertilizer*@
                                @*Total Fertilizer Expenses*@
                                <div class="align-items-center border-3 border-bottom mQuryMargin mb-2 row">
                                    <div class="col-5 mb-2">
                                        <div class="AccordInnerHead totalGren">
                                            Fertilizer Expenses
                                        </div>
                                    </div>
                                    <div class="col-6 InptRgt mb-2">
                                        <div class="row align-items-center">
                                            <div class="col-lg-10 col-8 totalGren">
                                                <span>$/acre</span>
                                                <span id="TotalFertilizerExpenses"></span>
                                                @Html.HiddenFor(x => x.TotalFertilizerExpenses)
                                            </div>
                                            <div class="col-lg-2 col-4">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row align-items-center mb-4">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Nitrogen (as N)
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Nitrogen (as N)"></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">
                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.Nitrogen, new { @id = "Nitrogen-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, onblur="sendInputValue(this)",  @class = "form-control disabler validator mask small-6-col", placeholder = "$", type = "text"  })
                                                </div>
                                            </div>
                                            <div class="col-2">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row align-items-center mb-4">
                                    <div class="col-md-4">
                                        <div class="AccordInnerHead d-flex align-items-center">
                                            Phosphorus (as P)
                                            <i class="bi bi-info-circle-fill ms-2 circlefill" data-toggle="tooltip" data-bs-placement="right" title="Phosphorus (as P)"></i>
                                        </div>
                                    </div>
                                    <div class="col-md-7 InptRgt">

                                        <div class="row align-items-center">
                                            <div class="col-2">
                                                <label class="float-end">$/acre</label>
                                            </div>
                                            <div class="col-5 p-0">
                                                <div class="input-group">

                                                    @Html.TextBoxFor(x => x.Phosphorus, new { @id = "Phosphorus-" + Model.Management_Zone_Id + "-" + Model.Year+ "-" +Model.ZydId, onblur="sendInputValue(this)", @class = "form-control disabler validator mask small-6-col", placeholder = "$", type = "text" })
                                                </div>
                                            </div>
                                            <div class="col-2">
                                            </div>
